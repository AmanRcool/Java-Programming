import java.util.*;
class AbcD{
public static void main(String args[]){
Scanner sc  = new Scanner(System.in);
System.out.println("Enter Number");
int n = sc.nextInt();
System.out.println("_________________");
char a = 'A';
char b = 'a';
for(int i=0;i<=n;i++){
for(int j=0;j<=i;j++){
if((i+j)%2==0)
System.out.print(a);
else
System.out.print(b);
a++;
b++;
}
System.out.println();
}
}

}

/**
output
Enter Number
4
________________
A
bC
DeF
gHiJ
KlMnO





*/
